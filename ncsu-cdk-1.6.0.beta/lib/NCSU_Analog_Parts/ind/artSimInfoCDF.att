        componentName     ind
        otherParameters   (l source qmode q f)
        instParameters    ((expr case((iPar 'qmode) ("ideal" '(l)) ("Constant Resistance" '(l q f)) ("Square Root" '(l q1 f)) ("Constant Q" '(l q2 f)))))
        propMapping       (nil q1 q q2 q f freq)
        termOrder         (PLUS MINUS)
        namePrefix        ""
    )
