    ;;; Simulator Information
    cdfId->simInfo = list( nil )
    cdfId->simInfo->auLvs = '( nil
        propMapping       nil
        netlistProcedure  ansLvsCompPrim
        instParameters    (l)
        componentName     ind
        termOrder         (PLUS MINUS)
        permuteRule       "(p PLUS MINUS)"
        namePrefix        "L"
    )
    cdfId->simInfo->cdsSpice = '( nil
        propMapping       nil
        netlistProcedure  ansSpiceCompPrim
        instParameters    (l ic)
        componentName     ind
        termOrder         (PLUS MINUS)
        namePrefix        "L"
        current           port
        termMapping       (nil PLUS "" MINUS "(FUNCTION minus(root(\"PLUS\")))")
        noPortDelimiter   t
        dcSens            nil
        acSens            nil
    )
    cdfId->simInfo->hpmns = '( nil
        propMapping       nil
        netlistProcedure  ansHpmnsCompPrim
        instParameters    (l)
        componentName     l
        termOrder         (PLUS MINUS)
        current           port
        termMapping       (nil PLUS "plus" MINUS "minus")
        namePrefix        "I"
    )
    cdfId->simInfo->hspiceS = '( nil
        propMapping       nil
        netlistProcedure  ansSpiceCompPrim
        instParameters    (l r tc1 tc2 scale m dtemp ic polyCoef c1 c2 c3 c4 c5 c6 c7 c8 c9 c10 c11 c12 c13 c14 c15 c16 c17 c18 c19 c20)
        componentName     hind
        termOrder         (PLUS MINUS)
        namePrefix        "L"
        current           port
        termMapping       (nil PLUS "" MINUS "(FUNCTION minus(root(\"PLUS\")))")
        noPortDelimiter   t
        dcSens            nil
        acSens            nil
    )
    cdfId->simInfo->libra = '( nil
        netlistProcedure  ansLibraCompPrim
        ver4NetProc       ansLibra4CompPrim
        componentName     (expr case((iPar 'qmode) ("ideal" 'IND) (("Constant Resistance" "Square Root" "Constant Q") 'INDQ)))
        otherParameters   (l source qmode q f)
        instParameters    ((expr case((iPar 'qmode) ("ideal" '(l)) ("Constant Resistance" '(l q f mod\=1 temp)) ("Square Root" '(l q f mod\=2 temp)) ("Constant Q" '(l q f mod\=3 temp)))))
        propMapping       (nil f freq)
        typeMapping       (nil f frequency mod\=1 nameOnly mod\=2 nameOnly mod\=3 nameOnly temp temp)
        termOrder         (PLUS MINUS)
        namePrefix        "I"
    )
    cdfId->simInfo->mharm = '( nil
